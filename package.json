{
  "name": "graphics-ts",
  "version": "1.1.0",
  "description": "A porting of purescript-{canvas, free-canvas, drawing} featuring fp-ts",
  "files": [
    "lib",
    "es6"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "config": {
    "side": [],
    "modules": [],
    "globals": []
  },
  "exports": {
    ".": {
      "require": "./build/cjs/index.js"
    },
    "./*": {
      "require": "./build/cjs/*.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "prettier": "prettier --list-different \"./{src,test,example}/**/*.ts\"",
    "fix-prettier": "prettier --write \"./{src,test,example}/**/*.ts\"",
    "clean": "rimraf build/* dist/*",
    "prebuild": "npm run clean",
    "build": "pnpm build-all && pnpm build-pack",
    "build-all": "tsc -b tsconfig.build-es6.json",
    "build-mjs": "pnpm babel build/esm --config-file ./.babel.mjs.json --out-dir build/mjs --source-maps --out-file-extension .mjs",
    "build-cjs": "pnpm babel build/esm --config-file ./.babel.cjs.json --out-dir build/cjs --source-maps --out-file-extension .js",
    "build-pack": "pnpm build-mjs && pnpm build-cjs && pnpm build-utils pack-v4",
    "postbuild": "prettier --write \"./{lib,es6}/**/*.ts\"",
    "start": "vite -c vite.config.js example/",
    "docs": "docs-ts",
    "import-path-rewrite": "import-path-rewrite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/graphics-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/graphics-ts/issues"
  },
  "homepage": "https://github.com/gcanti/graphics-ts",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@effect-ts/build-utils": "^0.40.7",
    "@effect-ts/core": "^0.60.5",
    "@effect/language-service": "^0.0.19",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "cpx": "^1.5.0",
    "docs-ts": "^0.5.1",
    "dtslint": "github:gcanti/dtslint",
    "eslint": "^8.33.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "glob": "^7.1.6",
    "jsdom": "^21.1.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vitest": "^0.28.3",
    "vitest-canvas-mock": "^0.2.2"
  },
  "tags": [
    "typescript",
    "functional-programming",
    "html-canvas",
    "canvas"
  ],
  "keywords": [
    "typescript",
    "functional-programming",
    "html-canvas",
    "canvas"
  ],
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@effect/io": "^0.1.9",
    "@fp-ts/core": "^0.1.1",
    "@fp-ts/data": "^0.1.1"
  }
}
